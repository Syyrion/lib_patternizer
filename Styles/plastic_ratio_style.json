{
    // Style data id
    "id": "plastic_ratio",

	// * Don't need hue options if there are no dynamic colors

    // * Don't need pulse options if there are no pulse colors

    // 3D options
    "3D_depth": 1,
    "3D_skew": 0.15,
    "3D_spacing": 2.5,
    "3D_darken_multiplier": 1.1,
    "3D_alpha_multiplier": 0.4,
    "3D_alpha_falloff": 15.0,
    "3D_override_color": [85, 95, 155, 255],

    // Main color
	// * The "main" tag is completely useless
	// * colors are not dynamic by default
	// * Pulse is by default [0, 0, 0, 0]
    "main": {"value": [0, 0, 0, 255]},
    "cap_color": {"legacy": true, "index": 0},

    // Background colors
	// * Dynamic tags aren't needed if "dynamic" is false
    "colors":
    [
        {"value": [200, 200, 200, 255]},
		{"value": [195, 195, 195, 255]},
		{"value": [190, 190, 190, 255]},
		{"value": [185, 185, 185, 255]},
		{"value": [180, 180, 180, 255]},
		{"value": [175, 175, 175, 255]},
		{"value": [170, 170, 170, 255]},
		{"value": [165, 165, 165, 255]},
		{"value": [160, 160, 160, 255]},
		{"value": [155, 155, 155, 255]}

    ]
}
